#!/usr/bin/env python3


import arrays
import convert
import fire
import log
import strings


class Util():
    '''
    custom utility functions for better shell experience.

    '''
    arrays = arrays.Array()
    convert = convert.Convert()
    log = log.Log()
    string = strings.String()


if __name__ == "__main__":
    fire.Fire(Util)
